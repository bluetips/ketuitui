@charset "UTF-8";

.accredit-view {
    position: fixed;
    top: 0;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.5);
}

.accredit-view-content {
    position: fixed;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    padding: 40rpx 40rpx;
    width: 580rpx;
    background: #fff;
    border-radius: 10rpx;
}