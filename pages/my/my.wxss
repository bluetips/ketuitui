@charset "UTF-8";

.my {
    background: #f3f3f3;
    padding-bottom: 30rpx;
    position: relative;
}

.notask {
    text-align: center;
    color: #999;
    line-height: 100rpx;
}

.mask {
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    opacity: 0.75;
    background-color: #000000;
    z-index: 98;
}

.promote_sn_box {
    position: absolute;
    z-index: 99;
    left: 50%;
    top: 50%;
}

.promote_sn_box .inner_box {
    width: 520rpx;
    height: 670rpx;
    left: -300rpx;
    top: -380rpx;
    position: absolute;
    background-color: #ffffff;
    border-radius: 16rpx;
    padding: 40rpx;
    text-align: center;
}

.promote_sn_box input {
    margin-top: 50rpx;
    border-bottom: 1px solid #ce3c39;
    line-height: 80rpx;
    height: 80rpx;
    color: #ce3c39;
    font-size: 36rpx;
}

.promote_sn_box button {
    margin-top: 50rpx;
}

.promote_sn_box .rule {
    text-align: left;
    margin-top: 50rpx;
    line-height: 48rpx;
    font-size: 24rpx;
}

.promote_sn_box .inviter image {
    width: 80rpx;
    height: 80rpx;
    border-radius: 50%;
    vertical-align: text-bottom;
}

.promote_sn_box .inviter view {
    font-size: 32rpx;
    line-height: 70rpx;
}

.card {
    margin: 20rpx 30rpx 20rpx 30rpx;
    box-sizing: border-box;
    width: 690rpx;
    border-radius: 20rpx;
    background: #fff;
}

.card-title {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    margin: 20rpx 0rpx;
    padding: 20rpx 35rpx;
    border-bottom: 1px solid #ebebeb;
}

.card-title-text {
    font-size: 35rpx;
}

.user {
    position: relative;
    padding: 30rpx 30rpx;
    background: #ff393b;
    height: 300rpx;
}

.user-message {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    color: #fff;
}

.user-message-avatar {
    width: 130rpx;
    border-radius: 50%;
    margin-right: 25rpx;
    height: 130rpx;
    border: 4rpx solid #fff;
}

.user-message-username {
    font-weight: 600;
}

.user-message text.iconfont {
    font-size: 60rpx;
    color: #cccccc;
    margin-left: 10rpx;
}

.user-message text.vip1 {
    font-size: 60rpx;
    color: #feaf2e;
}

.user-message-detail {
    margin-left: auto;
    font-size: 30rpx;
}

.user-wallet {
    position: absolute;
    box-sizing: border-box;
    padding: 35rpx;
    bottom: 0;
    height: 280rpx;
    width: 690rpx;
    background: #fff;
    transform: translateY(50%);
    border-radius: 20rpx;
}

.user-wallet-can {
    color: #999;
    font-size: 30rpx;
}

.user-wallet-can-num {
    display: inline-block;
    padding: 20rpx 0;
    font-weight: 600;
    -webkit-text-stroke-width: 0.2px;
}

.user-wallet-takeDepoist {
    position: absolute;
    right: 0;
    top: 50%;
    transform: translateY(-50%);
    display: inline-block;
    width: 170rpx;
    height: 65rpx;
    font-size: 28rpx;
    text-align: center;
    line-height: 65rpx;
    color: #fff;
    border-radius: 10rpx;
    background: #ff393b;
}

.user-wallet-hadTake {
    position: absolute;
    box-sizing: border-box;
    left: 0;
    bottom: 0;
    width: 100%;
    padding: 20rpx 35rpx;
    border-top: 1rpx solid #ebebeb;
    font-size: 26rpx;
    overflow: hidden;
    color: #999;
}

.user-wallet-hadTake-text1 {
    -webkit-text-stroke-width: 0.2px;
    float: left;
}

.user-wallet-hadTake-text2 {
    font-size: 26rpx;
    float: right;
}

.my-task {
    margin: 170rpx 30rpx 20rpx 30rpx;
    height: 300rpx;
}

.my-task-all {
    font-size: 30rpx;
    margin-left: auto;
    color: #999;
}

.my-service {
    height: 300rpx;
}

.my .process {
    text-align: center;
    font-size: 32rpx;
    color: #ce3c39;
}

.mine-item {
    width: 92.3%;
    background: #fff;
    margin: 172rpx auto 0;
    border-radius: 20rpx;
}

.mine-item .iconfont {
    color: #f9566e;
}

.items {
    width: 92.3%;
    background: #fff;
    margin: 30rpx auto 0 auto;
    border-radius: 20rpx;
}

.mine-title {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    flex-direction: row;
    -webkit-box-pack: justify;
    -ms-flex-pack: justify;
    justify-content: space-between;
    height: 78rpx;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    padding: 0 30rpx;
    border-bottom: 1rpx solid #f7f7f7;
    border-radius: 20rpx 20rpx 0 0;
}

.nouse {
    background: #999!important;
}

.task {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    flex-direction: row;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    padding: 20rpx;
    border-bottom: 2rpx dashed #ffffff;
}

.task:last-child {
    border-bottom: 0;
}

.task_center {
    font-size: 28rpx;
    text-align: center;
    padding: 20rpx 0;
    color: #0000FF;
}

.task_center icon {
    color: #0000FF!important;
}

.taskcont {
    width: 70%;
    margin-left: 20rpx;
}

.gettask {
    color: #ffc916;
    font-size: 32rpx;
}

.finish {
    color: green;
    font-size: 30rpx;
}

.tasktitle {
    font-weight: bold;
    font-size: 30rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.taskmoney {
    color: #666;
    font-size: 26rpx;
    margin-top: 8rpx;
}

.icondistrict {
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    display: -webkit-flex;
    -webkit-box-pack: justify;
    -ms-flex-pack: justify;
    justify-content: space-between;
    flex-wrap: wrap;
    padding: 30rpx 0 0;
    text-align: center;
}

.item-icon {
    width: 25%;
    text-align: center;
    margin-bottom: 50rpx;
}

.item-icon view {
    font-size: 28rpx;
}

.item-icon .iconfont {
    font-size: 60rpx;
    margin-bottom: 10rpx;
    color: #f9566e;
}

button.contact_btn {
    background: transparent;
    line-height: normal;
    font-size: inherit;
    padding: 0;
}

button.contact_btn::after {
    border: 0;
}

.nouse {
    background: #999;
}

page {
    height: 100%;
}

.bigprice {
    font-size: 58rpx;
    font-weight: bold;
}

.smallprice {
    font-size: 40rpx;
    font-weight: bold;
    color: #333;
}

.theme_vip1 {
    background-color: #ffffff;
}

.theme_vip1 .user {
    background-color: #322922;
}

.theme_vip1 .user-wallet {
    background-color: #dfc18f;
    color: #ffffff;
}

.theme_vip1 .user-wallet-can {
    color: #ffffff;
}

.theme_vip1 .user-wallet-hadTake {
    color: #ffffff;
}

.theme_vip1 .user-wallet .smallprice {
    color: #ffffff;
}

.theme_vip1 .user-wallet-takeDepoist {
    background-color: #584838;
}

.theme_vip1 .item-icon .iconfont,.theme_vip1 .mine-item .iconfont {
    color: #cea56d;
}

.theme_vip1 .mine-title {
    background-color: #dfc18f;
    color: #ffffff;
}

.theme_vip1 .mine-item,.theme_vip1 .items {
    background-color: #f8f3eb;
}